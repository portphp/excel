{
    "name": "portphp/excel",
    "description": "Excel reader and writer for Port",
    "license": "MIT",
    "homepage": "https://github.com/portphp",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        },
        {
            "name": "Markus Bachmann",
            "email": "markus.bachmann@bachi.biz"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/portphp/excel/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/portphp/portphp/issues",
        "source": "https://github.com/portphp/excel",
        "docs": "https://portphp.readthedocs.org"
    },
    "require": {
        "portphp/portphp": "^1.2.0",
        "phpoffice/phpexcel": "~1.8"
    },
    "autoload": {
        "psr-4": {
            "Port\\Excel\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Port\\Excel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "suggest": {
        "ext-zip": "To support writing of Excel files"
    }
}
